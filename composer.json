{
    "name": "jenky/hermes",
    "type": "library",
    "description": "Laravel guzzle manager",
    "keywords": [
        "laravel",
        "guzzle",
        "http",
        "http client",
        "http message",
        "PSR http",
        "psr-7",
        "jenky"
    ],
    "homepage": "https://github.com/jenky/hermes",
    "license": "MIT",
    "authors": [
        {
            "name": "Lynh",
            "email": "contact@lynh.me",
            "homepage": "https://github.com/jenky",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "illuminate/config": "^5.7|^6.0|^7.0",
        "illuminate/container": "^5.7|^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5|^4.0",
        "phpunit/phpunit": "^7.0|^8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Jenky\\Hermes\\": "src"
        },
        "files": [
            "src/functions.php",
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Jenky\\Hermes\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Jenky\\Hermes\\HermesServiceProvider"
            ],
            "aliases": {
                "Guzzle": "Jenky\\Hermes\\Facades\\Guzzle"
            }
        }
    },
    "suggest": {
        "symfony/psr-http-message-bridge": "Create Laravel response directly from PSR-7 HTTP message response"
    },
    "config": {
        "sort-packages": true
    }
}
