{
    "name": "jenky/guzzilla",
    "type": "library",
    "description": "Laravel guzzle manager",
    "keywords": [
        "laravel",
        "guzzle",
        "http",
        "http message",
        "PSR http",
        "psr-7",
        "jenky",
        "guzzila"
    ],
    "homepage": "https://github.com/jenky/guzzilla",
    "license": "MIT",
    "authors": [
        {
            "name": "Lynh",
            "email": "contact@lynh.me",
            "homepage": "https://github.com/jenky",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.1",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Jenky\\Guzzilla\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jenky\\Guzzilla\\": "tests"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Jenky\\Guzzilla\\GuzzillaServiceProvider"
            ]
        }
    },
    "suggest": {
        "symfony/psr-http-message-bridge": "Create Laravel response directly from PSR HTTP message response"
    },
    "config": {
        "sort-packages": true
    }
}
